{
  "name": "character-picker",
  "type": "module",
  "scripts": {
    "update-index": "node scripts/update-index.js",
    "update-version": "node scripts/update-version.js",
    "dev": "pnpm run update-index && vite",
    "build": "pnpm run update-index && vite build",
    "preview": "pnpm run build && vite preview",
    "deploy": "pnpm run prepublishOnly && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "build:node": "pnpm run update-index && vite build -c vite.config.node.ts",
    "start:node": "pnpm run build:node && node dist/node/entry-node.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm run update-version && pnpm run lint:fix && pnpm run build"
  },
  "dependencies": {
    "hono": "^4.11.4",
    "image-size": "^2.0.2",
    "uint8array-extras": "^1.5.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.21.0",
    "@eslint/js": "^10.0.0",
    "@hono/node-server": "^1.19.9",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@typescript-eslint/typescript-estree": "^8.53.0",
    "eslint": "^10.0.0",
    "globals": "^17.0.0",
    "jiti": "^2.6.1",
    "mime-types": "^3.0.2",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vite-ssr-components": "^0.5.2",
    "wrangler": "^4.59.2"
  }
}
